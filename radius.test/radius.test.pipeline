{
    "pipeline": [
        {
            "type": "readers.text",
            "filename": "radius.test.xyz"
        },
        {
            "type": "writers.gdal",
						"bounds": "([-100.5,100.5],[-100.5,100.5])",
            "resolution": 1,
            "window_size": 0,
            "output_type": "max,idw,count,stdev",
            "dimension": "Z",
            "filename": "radius.test.tif",
            "data_type": "float",
            "nodata": -9999
        },
        {
            "type": "writers.gdal",
						"bounds": "([-100.5,100.5],[-100.5,100.5])",
						"radius": 0.707,
            "resolution": 1,
            "window_size": 0,
            "output_type": "max,idw,count,stdev",
            "dimension": "Z",
            "filename": "radius.2.test.tif",
            "data_type": "float",
            "nodata": -9999
        }
    ]
}
